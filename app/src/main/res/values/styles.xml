<resources>
    
    <style name="AppTheme" parent="Theme.AppCompat.Light.NoActionBar">

    </style>

    <style name="bybutton">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginTop">10dp</item>
        <item name="android:background">@drawable/button</item>
        <item name="android:paddingBottom">8dp</item>
        <item name="android:paddingLeft">25dp</item>
        <item name="android:paddingRight">25dp</item>
        <item name="android:paddingTop">8dp</item>
        <item name="android:textSize">18sp</item>
        <item name="android:textColor">@color/white</item>
    </style>
    
    <style name="byTextView">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginTop">10dp</item>
        <item name="android:background">@drawable/button</item>
        <item name="android:paddingBottom">8dp</item>
        <item name="android:paddingLeft">25dp</item>
        <item name="android:paddingRight">25dp</item>
        <item name="android:paddingTop">8dp</item>
        <item name="android:textSize">18sp</item>
        <item name="android:textColor">@color/white</item>
    </style>

    <style name="style_common_listview">
        <item name="android:padding">0dp</item>
        <item name="android:horizontalSpacing">0dp</item>
        <item name="android:verticalSpacing">0dp</item>
        <item name="android:divider">@color/transparent</item>
        <item name="android:dividerHeight">@dimen/y20</item>
        <item name="android:scrollingCache">false</item>
        <item name="android:cacheColorHint">@android:color/transparent</item>
        <!--<item name="android:listSelector">@drawable/bg_white</item>-->
    </style>

</resources>